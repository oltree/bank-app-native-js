{
  "trailingComma": "es5",
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "arrowParens": "always",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/core/(.*)$",
    "^@/components/(.*)$",
    "^@/styles/(.*)$",
    "^@/config/(.*)$",
    "^@/utils/(.*)$",
    "^@/services/(.*)$",
    "^@/store/(.*)$",
    "^@/api/(.*)$",
    "(.scss)$",
    "^../(.*)",
    "^./(.*)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
